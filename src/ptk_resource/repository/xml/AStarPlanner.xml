<Pattern name="AStarPlanner">	<Agent name="AStarPlanner" extends="GenericAgent">		<Attribute name="graph" type="AStarGraph"/>		<Attribute name="starting_state" type="AStarNode"/>		<Attribute name="ending_state" type="AStarNode"/>		<Attribute name="path" type="AStarPath"/>		<AgentSetup>			<Code>AgentSetup@AStarPlanner</Code>		</AgentSetup>		<Method name="setupAStar" type="void">			<Visibility>private</Visibility>			<Code>setupAStar@AStarPlanner</Code>		</Method>			<Task name="InitialCondition" extends="OneShotTask"/>			<Task name="AStarPlannerTask" extends="OneShotTask">			<Attribute name="opened" type="List"/>			<Attribute name="closed" type="List"/>			<Method name="executeAStar" type="void">				<Visibility>private</Visibility>				<Code>executeAStar@AStarPlanner</Code>			</Method>			<Method name="heuristic" type="double">				<Visibility>private</Visibility>				<Argoment name="A" type="AStarNode"/>				<Argoment name="B" type="AStarNode"/>				<Code>heuristic@AStarPlanner</Code>			</Method>			<Method name="costAmong" type="double">				<Visibility>private</Visibility>				<Argoment name="A" type="AStarNode"/>				<Argoment name="B" type="AStarNode"/>				<Code>costAmong@AStarPlanner</Code>			</Method>			<Method name="neighboringNodes" type="List">				<Visibility>private</Visibility>				<Argoment name="A" type="AStarNode"/>				<Code>neighboringNodes@AStarPlanner</Code>			</Method>			<Method name="isInList" type="ListNode">				<Visibility>private</Visibility>				<Argoment name="list" type="List"/>				<Argoment name="A" type="AStarNode"/>				<Code>isInList@AStarPlanner</Code>			</Method>			<Method name="getBestCostNode" type="ListNode">				<Visibility>private</Visibility>				<Argoment name="list" type="List"/>				<Code>getBestCostNode@AStarPlanner</Code>			</Method>		</Task>	</Agent></Pattern>